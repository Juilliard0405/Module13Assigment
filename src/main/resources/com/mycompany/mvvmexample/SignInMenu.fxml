<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="467.0" prefWidth="496.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.mvvmexample.SignInMenuController">
   <children>
      <VBox alignment="CENTER_RIGHT" layoutX="7.0" layoutY="5.0" prefHeight="413.0" prefWidth="485.0" spacing="10.0">
         <children>
            <Label text="Sign in">
               <VBox.margin>
                  <Insets right="200.0" />
               </VBox.margin>
               <font>
                  <Font size="29.0" />
               </font>
            </Label>
            <HBox prefHeight="0.0" prefWidth="465.0" />
            <TextField fx:id="username" alignment="TOP_LEFT" promptText="Username" />
            <TextField fx:id="password" promptText="Password" />
            <HBox prefHeight="23.0" prefWidth="465.0">
               <children>
                  <Label onMouseClicked="#switchSignUpMenu" prefHeight="17.0" prefWidth="120.0" text="Create Account " textFill="#4a7ce6" />
               </children>
            </HBox>
            <HBox prefHeight="32.0" prefWidth="465.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#SignInButton" text="Login" textAlignment="CENTER" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
